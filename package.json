{
  "name": "bilko-robot-ui",
  "productName": "bilko-robot-ui",
  "version": "0.4.0",
  "description": "UI application for Lynca Robot Controller users.",
  "author": {
    "name": "Bilko Bilgisayar Otomasyon ve Kontrol AS",
    "email": "bilko@bilko.com.tr",
    "url": "http://www.bilko-automation.com/"
  },
  "license": "UNLICENSED",
  "keywords": [
    "bilko-robot-ui"
  ],
  "icon": "public/app.png",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.3",
    "@testing-library/user-event": "^7.1.2",
    "ace-builds": "^1.4.12",
    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-ace": "^9.3.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.11.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.3.0",
    "react-simple-keyboard": "^2.5.169",
    "react-spinners": "^0.10.6",
    "react-toastify": "^7.0.3",
    "react-tooltip": "^4.2.15",
    "react-transition-group": "^4.4.1",
    "reconnecting-websocket": "^4.4.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "three": "^0.125.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "pack-lin32": "electron-packager . bilko-robot-ui --electron-version=11.2.3 --overwrite --asar --platform=linux --arch=ia32 --icon=./public/app.png --prune=true --out=dist",
    "pack-lin64": "electron-packager . bilko-robot-ui --electron-version=11.2.3 --overwrite --asar --platform=linux --arch=x64 --icon=./public/app.png --prune=true --out=dist",
    "pack-win64": "electron-packager . bilko-robot-ui --electron-version=11.2.3 --overwrite --asar --platform=win32 --arch=x64 --icon=./public/app.png --prune=true --out=dist",
    "pack-armv7l": "electron-packager . bilko-robot-ui --electron-version=11.2.3 --overwrite --asar --platform=linux --arch=armv7l --icon=./public/app.png --prune=true --out=dist",
    "dist-deb32": "electron-installer-debian --src dist/bilko-robot-ui-linux-ia32/ --arch i386 --dest dist/installer",
    "dist-deb64": "electron-installer-debian --src dist/bilko-robot-ui-linux-x64/ --arch amd64 --dest dist/installer",
    "dist-win64": "electron-installer-windows --src dist/bilko-robot-ui-win32-x64/ --dest dist/installer/",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000/ && SVGA_VGPU10=0 NODE_ENV=dev electron .\"",
    "electron-nginx": "electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "main": "electron.js"
}
